pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                sh 'python3 kreis.py'
            }
        }
        stage('Hello2') {
            steps {
                echo 'Hello World2'
                 sh 'python3 -m unittest kreis.py'//python code ausf√ºhren mit sh '...'
            }
           

        }
        stage('Hello3') {
            steps {
                echo 'Hello World3'
            }
        }
    }
}

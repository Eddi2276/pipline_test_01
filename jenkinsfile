pipeline {
    agent any

    stages {
        stage('build kreis.py') {
            steps {
                echo 'build kreis'
                sh 'python3 kreis.py'
            }
        }
        stage('build test_kreis.py') {
            steps {
                echo 'test kreis'
                 sh 'python3 -m unittest kreis.py'//python code ausf√ºhren mit sh '...'
            }
           

        }
        stage('change to cfiles') {
            steps {
                dir('cfiles') {
                    sh'gcc liste.c'
                    echo'change to cfiles sucsessfull'
                    sh'./a.out'
                    echo'c file output sucsessfull'
                }
            }
        }
    }
}
